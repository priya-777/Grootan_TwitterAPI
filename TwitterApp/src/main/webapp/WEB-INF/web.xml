<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
  	<servlet-name>GetInfo</servlet-name>
  	<display-name>GetInfo</display-name>
  	<description></description>
  	<servlet-class>com.TwitterAPI.Controllers.GetInfo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Tweets</servlet-name>
  	<display-name>Tweets</display-name>
  	<description></description>
  	<servlet-class>com.TwitterAPI.DAO.Tweets</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>GetInfo</servlet-name>
  	<url-pattern>/GetInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Tweets</servlet-name>
  	<url-pattern>/Tweets</url-pattern>
  </servlet-mapping>
</web-app>
